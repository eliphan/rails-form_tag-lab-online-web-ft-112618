<%= form_tag students_path do %>
  <p>Student Form</p>
  <label>First name:</label>
  <%= text_field_tag :'students[first_name]' %>
  <%= text_field_tag :'post[title]' %><br>
  <br>
  <label>Last name:</label>
  <%= text_field_tag :'students[last_name]' %>
  <br>
  <%= submit_tag "Submit" %>
<% end %>